# Z.AI API Key (optional - for GLM models)
ZAI_API_KEY=your_zai_api_key_here

# OpenRouter API Key (optional - for Claude and other models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Rate Limiting
# Maximum requests per minute per IP address
RATE_LIMIT_RPM=30

# Input Limits
# Maximum total characters allowed across all messages in a request
MAX_INPUT_CHARS=10000

# Figma MCP Integration (optional - for Figma API access)
# Get your token at: https://www.figma.com/developers/api#access-tokens
FIGMA_API_KEY=your_figma_api_key_here
